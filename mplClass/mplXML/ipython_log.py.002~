import numpy as N
_ip.magic("logstart ")

import obj2xml
a = N.arange(20)
b = N.arange(20)
ax = gca()
v = ax.vlines(a,0,b)
draw()
ans = obj2xml.Object2Xml(v)
ans = obj2xml.Object2Xml(v, 'vlines')
ans
ansStr = ans.getXMLString()
ansStr
print ansStr
ansStr = ans.getXML()
ans.xmlobj
ans._doc
ans.saveXML('v.xml')
v
ans.print_xml('v.xml')
import obj2xml
reload(obj2xml)
ans = obj2xml.Object2Xml(v, 'vlines')
ansV = ans.getXML(v, 'vlines')
ans.getXMLString()
ans._vars
import fig2xml
ans = fig2xml.Figure2XML(v, 'vlines')
ans.getXMLString()
reload(fig2xml)
ans = fig2xml.Figure2XML(v, 'vlines')
exit()
