  <match>
    <param name="tolerance" value="0.200000" type="float" />
    <param name="units" value="Da" type="str" />
    <param name="ignoreCharge" value="0" type="int" />
  </match>

  <processing>
    <crop>
      <param name="lowMass" value="500" type="int" />
      <param name="highMass" value="5000" type="int" />
    </crop>
    <smoothing>
      <param name="method" value="SG" type="str" />
      <param name="windowSize" value="0.300000" type="float" />
      <param name="cycles" value="2" type="int" />
    </smoothing>
    <baseline>
      <param name="segments" value="10" type="int" />
      <param name="offset" value="0.000000" type="float" />
      <param name="smooth" value="1" type="int" />
    </baseline>
    <peakpicking>
      <param name="peakWidth" value="0.250000" type="float" />
      <param name="snTreshold" value="3.000000" type="float" />
      <param name="absIntTreshold" value="0.000000" type="float" />
      <param name="relIntTreshold" value="0.000000" type="float" />
      <param name="smoothing" value="0" type="str" />
      <param name="pickingHeight" value="0.750000" type="float" />
      <param name="adaptiveNoise" value="0" type="int" />
      <param name="deisotoping" value="1" type="int" />
    </peakpicking>
    <deisotoping>
      <param name="maxCharge" value="1" type="int" />
      <param name="massTolerance" value="0.100000" type="float" />
      <param name="intTolerance" value="0.500000" type="float" />
      <param name="removeIsotopes" value="1" type="int" />
      <param name="removeUnknown" value="1" type="int" />
    </deisotoping>
  </processing>

  <calibration>
    <param name="fitting" value="quadratic" type="str" />
    <param name="tolerance" value="50.000000" type="float" />
    <param name="units" value="ppm" type="str" />
    <param name="statCutOff" value="800" type="int" />
  </calibration>

  <sequence>
    <digest>
      <param name="maxMods" value="1" type="int" />
      <param name="maxCharge" value="1" type="int" />
      <param name="massType" value="0" type="int" />
      <param name="enzyme" value="Trypsin" type="str" />
      <param name="miscl" value="1" type="int" />
      <param name="lowMass" value="500" type="int" />
      <param name="highMass" value="5000" type="int" />
      <param name="allowMods" value="0" type="int" />
      <param name="listFormat" value="b.S.a [m]" type="str" />
      <param name="matchFormat" value="[r] b.S.a [m]" type="str" />
    </digest>
    <fragment>
      <param name="maxMods" value="1" type="int" />
      <param name="maxCharge" value="1" type="int" />
      <param name="massType" value="1" type="int" />
      <param name="fragments" value="a;b;y;-NH3;-H2O" type="str" />
      <param name="filterFragments" value="1" type="int" />
      <param name="listFormat" value="b.S.a [m]" type="str" />
      <param name="matchFormat" value="[f] b.S.a [m]" type="str" />
