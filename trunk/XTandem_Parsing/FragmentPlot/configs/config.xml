    <search>
      <param name="maxMods" value="1" type="int" />
      <param name="maxCharge" value="1" type="int" />
      <param name="massType" value="0" type="int" />
      <param name="enzyme" value="Trypsin" type="str" />
      <param name="tolerance" value="0.200000" type="float" />
      <param name="units" value="Da" type="str" />
      <param name="listFormat" value="b.S.a [m]" type="str" />
    </search>
  </sequence>

